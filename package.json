{
  "name": "google-apps-script-project",
  "version": "1.0.0",
  "description": "Clasp project for multiple deployments",
  "main": "index.js",
  "scripts": {
    "login": "clasp login",
    "pull": "clasp pull",
    "push:master": "clasp push && git add . && git commit -m \"Master Deploy\" && git push",
    "push:all": "node multi-deploy.js && git add . && git commit -m \"Fleet Deploy\" && git push",
    "docs:install": "cd docs-src && npm install",
    "docs:dev": "cd docs-src && npm run dev",
    "docs:build": "cd docs-src && npm run build",
    "docs:preview": "cd docs-src && npm run preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/clasp": "^2.4.2"
  }
}